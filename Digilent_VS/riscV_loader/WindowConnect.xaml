<Window x:Class="riscV_loader.WindowConnect"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:riscV_loader"
        mc:Ignorable="d"
        Title="Serial connection" Height="155" Width="220" WindowStyle="ToolWindow" Loaded="WindowConnect_Loaded">
    <Canvas>
        <ComboBox Name="comboBoxPorts" Width="170" Canvas.Top="15" Canvas.Left="10" />

        <ComboBox Name="comboBoxBaud" Width="170" Canvas.Top="50" Canvas.Left="10" />

        <Button Name="buttonConnect" Width="60" Height="22" Canvas.Top="90" Canvas.Left="140" Click="ButtonConnect_Click">
            Connect
        </Button>
    </Canvas>
</Window>
